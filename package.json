{
	"name": "tracepoint-backend",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"start": "bun src/index.ts",
		"db:generate": "bunx prisma generate",
		"db:migrate": "bunx prisma migrate dev",
		"db:push": "bunx prisma db push",
		"db:seed": "bun src/db/seed.ts",
		"db:studio": "bunx prisma studio",
		"openapi": "bun scripts/generate-openapi.ts",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"format": "biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.1",
		"@types/bun": "latest",
		"@types/pg": "^8.16.0",
		"prisma": "^7.4.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@bogeychan/elysia-logger": "^0.1.10",
		"@elysiajs/cors": "^1.4.1",
		"@elysiajs/openapi": "^1.4.14",
		"@prisma/adapter-pg": "^7.4.0",
		"@prisma/client": "^7.4.0",
		"elysia": "^1.4.25",
		"pg": "^8.18.0",
		"pino-pretty": "^13.1.3"
	}
}
